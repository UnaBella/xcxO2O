/**show.wxss**/
.commodityImages {
  width: 100%;
  height: 378px;
  position: relative;
}
.commodityImages swiper, .commodityImages swiper-item, .commodityImages block, .commodityImages image{
  width: 100%;
  height: 100%;
}
.commodityTitles{
  width: 100%;
  height: 40px;
  line-height: 40px;
  text-indent: 1em;
  position: absolute;
  bottom: 0;
  left: 0;
  opacity: 0.6;
  background-color: #000000;

  overflow: hidden;
}
.commodityTitlesBtn{
  font-size: 14px;
  color: #ffffff;
  float: right;
  padding-right: 10px;
}